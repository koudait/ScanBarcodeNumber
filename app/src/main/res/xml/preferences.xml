<PreferenceScreen
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory
        app:iconSpaceReserved="false"
        app:title="@string/preference_category_info"
        >
        <Preference
            app:enableCopying="true"
            app:iconSpaceReserved="false"
            app:key="VERSION"
            app:persistent="false"
            app:title="@string/preference_title_version"
            />
    </PreferenceCategory>
</PreferenceScreen>
